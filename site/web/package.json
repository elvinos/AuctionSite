{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "settings": {
    "import/core-modules": [],
    "import/ignore": [
      "node_modules",
      "\\.(coffee|scss|css|less|hbs|svg|json)$"
    ]
  },
  "scripts": {
    "build": "webpack --progress --config resources/build/webpack.config.js",
    "build:production": "webpack --progress -p --config resources/build/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config resources/build/webpack.config.js",
    "start": "webpack --hide-modules --watch --config resources/build/webpack.config.js",
    "rmdist": "rimraf dist"
  },
  "engines": {
    "node": ">= 6.9.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.23.5",
    "browsersync-webpack-plugin": "^0.6.0",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-webpack-plugin": "^1.6.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "bootstrap-4": "^4.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "1.12.4 - 3",
    "jquery-easing": "^0.0.1",
    "popper.js": "~1.12.3",
    "start": "^5.1.0",
    "yarn": "^1.3.2"
  }
}
